{
  "openapi": "3.0.4",
  "info": {
    "title": "ExcelTool",
    "version": "1.0"
  },
  "paths": {
    "/api/excel/upload": {
      "post": {
        "tags": [
          "ExcelUpload"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "TableName": {
                    "type": "string"
                  },
                  "File": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "TableName": {
                  "style": "form"
                },
                "File": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ExcelUploadResult"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ExcelUploadResult"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ExcelUploadResult"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ExcelUploadResult": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "insertedRows": {
            "type": "integer",
            "format": "int32"
          },
          "errors": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  },
  "tags": [
    {
      "name": "ExcelUpload"
    }
  ]
}